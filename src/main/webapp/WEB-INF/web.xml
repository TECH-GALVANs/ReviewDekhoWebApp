<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProductController</servlet-name>
  	<display-name>ProductController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.product.controller.ProductController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReviewController</servlet-name>
  	<display-name>ReviewController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.review.controller.ReviewController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.user.controller.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LikeController</servlet-name>
  	<display-name>LikeController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.like.controller.LikeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReportController</servlet-name>
  	<display-name>ReportController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.report.controller.ReportController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CommentController</servlet-name>
  	<display-name>CommentController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.controller.CommentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductImageController</servlet-name>
  	<display-name>ProductImageController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.image.controller.ProductImageController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReviewerController</servlet-name>
  	<display-name>ReviewerController</display-name>
  	<description></description>
  	<servlet-class>com.reviewdekho.reviewer.controller.ReviewerController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductController</servlet-name>
  	<url-pattern>/ProductController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReviewController</servlet-name>
  	<url-pattern>/ReviewController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LikeController</servlet-name>
  	<url-pattern>/like</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReportController</servlet-name>
  	<url-pattern>/ReportController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CommentController</servlet-name>
  	<url-pattern>/CommentController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductImageController</servlet-name>
  	<url-pattern>/ProductImageController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReviewerController</servlet-name>
  	<url-pattern>/ReviewerController</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>views/index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
